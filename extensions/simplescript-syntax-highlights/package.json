{
    "name": "simplescript-syntax-highlights",
    "publisher": "GodOfProgramming",
    "repository": "https://github.com/GodOfProgramming/simple_script_rust",
    "displayName": "SimpleScript Syntax Highlights",
    "description": "Syntax highlights for SimpleScript",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "simplescript",
                "aliases": [
                    "SimpleScript",
                    "simplescript"
                ],
                "extensions": [
                    ".ss"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "simplescript",
                "scopeName": "source.ss",
                "path": "./syntaxes/simplescript.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.14.0"
    }
}
